               	end
               
               	//Print the END_OF_DOCUMENT block
               	INTERNAL PROCEDURE iPrint_END_OF_DOCUMENT()
               	X0,Y0 are real = (iXPos(), iYPos())
               	//Print the PoweredWx control
               	iXPos(X0 + 110.440000);iYPos(Y0 + 0.000000)
