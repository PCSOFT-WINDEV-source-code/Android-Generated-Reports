               	iPrintZone("Developed with WINDEV Mobile",iXPos(),iYPos()+1.000000,iXPos()+58.422000,iYPos()+4.556000,iRight+iCenterV)
               	iXPos(X0);iYPos(Y0)
               	end
               
               
               //Print
               sPDF is string
               if SysStatusExternalStorage(1) = sseAvailable then
               	sPDF = SysDirExternalStorage(1, sseAppDocument)
               else
               	sPDF = fCurrentDir()
               end
               sPDF += [fSep] + "RPT_Product.pdf"
               iDestination(iGenericPDF, sPDF)
               
               iParameter(iPaperSize,iPaperSize_A4)
               iParameter(iOrientation,iOrientation_Portrait)
               iDeclareFonts()
               //Position on margins
               iXPos(20.000000);iYPos(20.000000)
               iPrint_START_OF_DOCUMENT()
               iYPos(iYPos() + 20.217000)
               iPrint_PAGE_HEADER()
               iYPos(iYPos() + 12.170000)
               iPrint_BODY()
               iYPos(iYPos() + 52.424000)
               iPrint_END_OF_DOCUMENT()
               iYPos(iYPos() + 7.085000)
               
               iEndPrinting()
               ShellExecute(sPDF)
