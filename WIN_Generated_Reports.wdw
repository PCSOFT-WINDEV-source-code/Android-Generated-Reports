               	iCreateFont(3,9,iNormal,"Trebuchet MS",RGB(32,33,36))
               	iCreateFont(4,8,iNormal,"Arial",RGB(48,48,48))
               	end
               
               	//Print the START_OF_DOCUMENT block
               	INTERNAL PROCEDURE iPrint_START_OF_DOCUMENT()
               	X0,Y0 are real = (iXPos(), iYPos())
               	//Print the DOCTITLE control
               	iXPos(X0 + 64.573000);iYPos(Y0 + 0.000000)
               	iFont(1)
               	iPrintZone("Products",iXPos()+1.000000,iYPos()+1.000000,iXPos()+39.854000,iYPos()+8.701000,iCenterH+iCenterV)
               	//Print the DATESYS control
               	iXPos(X0 + 132.224000);iYPos(Y0 + 0.000000)
               	iPrintZone(DateToString(DateSys(), "MM/DD/YYYY"),iXPos(),iYPos()+1.000000,iXPos()+36.815000,iYPos()+8.701000,iRight+iCenterV)
               	//Print the IMG_Logo control
               	iXPos(X0 + 0.000000);iYPos(Y0 + 0.000000)
               	iPrintImage("Material Design Blue 2_Logo.png", iXPos(),iYPos(),iXPos()+34.769000,iYPos()+20.217000,iNormalDisplay)
               	iXPos(X0);iYPos(Y0)
               	end
               
               	//Print the PAGE_HEADER block
               	INTERNAL PROCEDURE iPrint_PAGE_HEADER()
