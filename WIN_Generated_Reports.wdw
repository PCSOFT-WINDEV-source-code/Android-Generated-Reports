               		iPrintZone("Quantity :",iXPos()+1.000000,iYPos()+1.000000,iXPos()+21.000000,iYPos()+5.085000,iLeft+iCenterV)
               		//Print the STC_Quantity1 control
               		iXPos(X0 + 50.693000);iYPos(Y0 + 19.170000)
               		iFont(3)
               		iPrintZone(Product.Quantity,iXPos()+1.000000,iYPos()+1.000000,iXPos()+24.800000,iYPos()+5.085000,iLeft+iCenterV)
               		//Print the STC_Bar_code1 control
               		iXPos(X0 + 110.528000);iYPos(Y0 + 5.997000)
