               	iXPos(X0 + 4.057000);iYPos(Y0 + 2.999000)
               	iFont(2)
               	iPrintZone("Products",iXPos()+1.000000,iYPos()+1.000000,iXPos()+29.800000,iYPos()+6.232000,iLeft+iCenterV)
               	iXPos(X0);iYPos(Y0)
               	end
               
               	//Print the BODY block
               	INTERNAL PROCEDURE iPrint_BODY()
               		for each Product 
               		X0,Y0 are real = (iXPos(), iYPos())
               		//Print the STC_Name1 control
               		iXPos(X0 + 27.693000);iYPos(Y0 + 6.000000)
